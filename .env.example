# .env.example

# PostgreSQL - Connection string for the backend service running in Docker
DATABASE_URL=postgresql+psycopg_async://postgres:postgres@db:5432/goodstewards

# FastAPI - Secret key for signing JWTs
SECRET_KEY=a_very_secret_key_that_should_be_in_env_for_prod
ACCESS_TOKEN_EXPIRE_MINUTES=43200

# BAML - URL for the BAML language model proxy
BAML_CLIENT_URL=http://localhost:2022

# Cloudflare R2 - Credentials for object storage
R2_ACCESS_KEY_ID=
R2_SECRET_ACCESS_KEY=
R2_ENDPOINT_URL=
R2_BUCKET_NAME=goodstewards-receipts
